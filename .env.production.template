# Supabase PostgreSQL Connection
# Get this from your Supabase project settings > Database
SUPABASE_DATABASE_URL=postgresql://postgres.[PROJECT_ID]:[PASSWORD]@aws-0-us-east-1.pooler.supabase.com:6543/postgres

# DigitalOcean Redis (your existing cluster)
# Replace with your actual Redis connection string
DO_REDIS_URL=rediss://default:[REDIS_PASSWORD]@[REDIS_HOST]:25061/0

# Open WebUI Configuration
WEBUI_SECRET_KEY=generate-strong-secret-key-here
# This will be generated after Open WebUI setup
OPENWEBUI_API_KEY=

# FileBrowser Authentication
FB_USERNAME=admin
FB_PASSWORD=change-this-secure-password

# Code Server Authentication
CODE_SERVER_PASSWORD=change-this-secure-password

# Timezone
TZ=America/New_York